<!doctype html>
<html lang="en">

{{>_head.mustache }}

<body class="js-enabled">
<script type="text/javascript">
    document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');
    </script>
<header class="header" id="header">
    <div style="display: block;">
        <!--start header-->
        <header role="banner" id="global-header" class="with-propositionx">
            <div class="header-wrapper">
                <div class="header-global">
                    <div class="header-logo">
                        <a href="http://localhost:8089/springmvc/dispatcher/login" title="Validation screen" id="logo" class="content">
                            <img src="images/header_ho_logo.png" alt="">
                        </a>
                    </div>
                    <h1 class="bold-medium"><abbr title="Dashboard">Asylum Support</abbr></h1>
                </div>
            </div>
            <div class="header-bar"></div>
        </header>
        <!--end header-->
    </div>
</header>
<header class="header-claim-data"></header>
<main id="page-container" role="main" aria-live="polite">
    <div class="main-area">
        <div>
            <div id="notification">
                <div style="display: block;"></div>
            </div>

            <div id="questions">

                <div style="display: block;">
                    <div class="start-content">
                        <div class="text-content">

                            <h1 class="heading-large">Create an account</h1>

                            <div class="form-group">
                                <form action="submitRegistration" method="post">
                                    <fieldset>

                                        <legend class="form-label" for=""></legend>

                                        <label for="noLongerRequestingIA">
                                           First name <input type="text" name ="userName" />
                                        </label>

                                        <label for="noLongerRequestingIA">
                                            Last name <input type="text" />
                                        </label>

                                        <label for="noLongerRequestingIA">
                                            Email address <input type="text" />
                                        </label>

                                        <label for="noLongerRequestingIA">
                                            Password <input type="password" name ="password" />
                                        </label>

                                        <input type="submit" value="Submit" class="button">

                                    </fieldset>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

{{> _footer.mustache }}

{{> _local-scripts.mustache }}

</body>
</html>


